{
    "env": {
        "browser": true,
        "commonjs": true,
        "es2021": true
    },
    "extends": "airbnb-base",
    "overrides": [
       
    ],
    "parserOptions": {
        "ecmaVersion": "latest"
    },
    "rules": {  
